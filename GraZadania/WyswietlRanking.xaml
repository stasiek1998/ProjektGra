<Page x:Class="GraZadania.WyswietlRanking"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GraZadania"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="WyswietlRanking">

    <Grid>
        <Button Name="ZagrajPonownie" Click="Button_Click" Content="Zagraj Ponownie" HorizontalAlignment="Left" Height="37" Margin="259,268,0,0" VerticalAlignment="Top" Width="223"/>
        <Button Content="Wyświetl Ranking" Click="Button_Click_1" HorizontalAlignment="Left" Height="37" Margin="30,268,0,0" VerticalAlignment="Top" Width="224"/>
        <DataGrid Name="myDataGrid" HorizontalAlignment="Left" Height="239" Margin="30,24,0,0" VerticalAlignment="Top" Width="452">

        <DataGrid.Columns>
                <DataGridTextColumn Header="Imie" Width="225" Binding="{Binding Imie}"/>
                <DataGridTextColumn Header="Punkty" Width="220" Binding="{Binding Punkty}"/>
        </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
